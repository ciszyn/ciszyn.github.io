{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "foo",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "foo",
            "kebab-case"
        ]
    }
}